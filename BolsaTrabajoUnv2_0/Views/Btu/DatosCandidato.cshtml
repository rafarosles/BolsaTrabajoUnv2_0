                    Cursos / Talleres
                    <span class="badge badge-primary badge-pill">
                        <i id="curtall1" class="fas fa-check"></i>
                        <i id="curtall2" style="display:none" class="fas fa-exclamation"></i>
                    </span>
                </li>
            </ul>
        </div>
        <div class="col-md-10">
            <ul class="nav nav-tabs" id="tabInfoPersonal" role="tablist">
                <li class="nav-item">
                    <a class="nav-link active" id="infoPersonal-tab" data-toggle="tab" href="#infoPer" role="tab" aria-controls="infoPer" aria-selected="true">Información Personal</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" id="estuAcad-tab" data-toggle="tab" href="#estAcad" role="tab" aria-controls="estudiosAcad" aria-selected="false">Estudios Académicos</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" id="softIdio-tab" data-toggle="tab" href="#softIdio" role="tab" aria-controls="softIdio" aria-selected="false">Software / Idiomas</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" id="expProf-tab" data-toggle="tab" href="#expProf" role="tab" aria-controls="expProf" aria-selected="false">Experiencia Profesional</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" id="curTall-tab" data-toggle="tab" href="#curTall" role="tab" aria-controls="curTall" aria-selected="false">Cursos / Talleres</a>
                </li>
            </ul>

            <div class="tab-content" id="myTabContent">
                <div class="tab-pane fade show active" id="infoPer" role="tabpanel" aria-labelledby="home-tab">
                    <form name="myForm">
                        <div class="row">
                            <div class="col-md-6 py-3">
                                <div class="input-group mb-3">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text" id="basic-addon1">Matricula</span>
                                    </div>
                                    <input ng-model="ctrl.Matricula" id="Matricula" name="Matricula" ng-change="ctrl.GlobalInfoPersonal()" type="text" class="form-control" placeholder="Matricula" aria-label="Matricula" aria-describedby="basic-addon1" style="max-width:100%" required>
                                </div>
                                <span style="color:red" ng-show="myForm.Matricula.$invalid" />
                                <span ng-show="myForm.Matricula.$error.required">*Matricula requerida</span>
                            </div>
                            <div class="col-md-6 py-3">
                                <div class="input-group mb-3">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text" id="basic-addon1">Nombre(s)</span>
                                    </div>
                                    <input ng-model="ctrl.NombreCandidato" id="NombreCandidato" name="NombreCandidato" ng-change="ctrl.GlobalInfoPersonal()" type="text" class="form-control" placeholder="Nombre(s)" aria-label="Nombre" aria-describedby="basic-addon1" style="max-width:100%" required>
                                </div>
                                <span style="color:red" ng-show="myForm.NombreCandidato.$invalid" />
                                <span ng-show="myForm.NombreCandidato.$error.required">*Nombre requerido</span>
                            </div>
                            <div class="col-md-6 py-3">
                                <div class="input-group mb-3">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text" id="basic-addon1">Apellido Paterno</span>
                                    </div>
                                    <input ng-model="ctrl.ApePatCandidato" id="ApePatCandidato" name="ApePatCandidato" ng-change="ctrl.GlobalInfoPersonal()" type="text" class="form-control" placeholder="Apellido Paterno" aria-label="ApellidoPaterno" aria-describedby="basic-addon1" style="max-width:100%" required>
                                </div>
                                <span style="color:red" ng-show="myForm.ApePatCandidato.$invalid" />
                                <span ng-show="myForm.ApePatCandidato.$error.required">*Apellido Paterno requerido</span>
                            </div>
                            <div class="col-md-6 py-3">
                                <div class="input-group mb-3">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text" id="basic-addon1">Apellido Materno</span>
                                    </div>
                                    <input ng-model="ctrl.ApeMatCandidato" id="ApeMatCandidato" name="ApeMatCandidato" ng-change="ctrl.GlobalInfoPersonal()" type="text" class="form-control" placeholder="Apellido Materno" aria-label="ApellidoMaterno" aria-describedby="basic-addon1" style="max-width:100%" required>
                                </div>
                                <span style="color:red" ng-show="myForm.ApeMatCandidato.$invalid" />
                                <span ng-show="myForm.ApeMatCandidato.$error.required">*Apellido Materno requerido</span>
                            </div>
                            <div class="col-md-6 py-3">
                                <div class="input-group mb-3">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text" id="basic-addon1">Fecha de Nacimiento</span>
                                    </div>
                                    <input ng-model="ctrl.FecNac" id="FecNac" name="FecNac" ng-change="ctrl.GlobalInfoPersonal()" type="date" class="form-control" placeholder="" aria-label="FechaNacimiento" aria-describedby="basic-addon1" style="max-width:100%" required>
                                </div>
                                <span style="color:red" ng-show="myForm.FecNac.$invalid" />
                                <span ng-show="myForm.FecNac.$error.required">*Fecha de Nacimiento requerida</span>
                            </div>
                            <div class="col-md-6 py-3">
                                <div class="input-group mb-3">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text" id="basic-addon1">Estado</span>
                                    </div>
                                    <select name="Estado" id="Estado" ng-model="ctrl.Estado" ng-change="ctrl.GlobalInfoPersonal()" class="custom-select" ng-options="list.Id as list.Descripcion for list in ctrl.listEstados" ng-click="ctrl.ComboMunicipios()" style="max-width:100%" required></select>
                                </div>
                                <span style="color:red" ng-show="myForm.Estado.$invalid" />
                                <span ng-show="myForm.Estado.$error.required">*Estado requerido</span>
                            </div>
                            <div class="col-md-6 py-2">
                                <div class="input-group mb-3">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text" id="basic-addon1">Municipio</span>
                                    </div>
                                    <select name="Municipio" id="Municipio" ng-change="ctrl.GlobalInfoPersonal()" ng-model="ctrl.Municipio" class="custom-select" ng-options="list.Id as list.Descripcion for list in ctrl.listMunicipios" style="max-width:100%" required></select>
                                </div>
                                <span style="color:red" ng-show="myForm.Municipio.$invalid" />
                                <span ng-show="myForm.Municipio.$error.required">*Municipio requerido</span>
                            </div>
                            <div class="col-md-12 py-2">
                                <div class="input-group mb-3">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text" id="basic-addon1">Domicilio</span>
                                    </div>
                                    <input ng-model="ctrl.Domicilio" id="Domicilio" name="Domicilio" type="text" ng-change="ctrl.GlobalInfoPersonal()" class="form-control" placeholder="Domicilio" aria-label="Domicilio" aria-describedby="basic-addon1" style="max-width:100%" required>
                                </div>
                                <span style="color:red" ng-show="myForm.Domicilio.$invalid" />
                                <span ng-show="myForm.Domicilio.$error.required">*Domicilio requerido</span>
                            </div>
                            <div class="col-md-6 py-2">
                                <div class="input-group mb-3">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text" id="basic-addon1">Teléfono Celular</span>
                                    </div>
                                    <input ng-model="ctrl.TelCel" id="TelCel" name="TelCel" type="text" ng-change="ctrl.GlobalInfoPersonal()" class="form-control" placeholder="Teléfono Celular" aria-label="TeléfonoCelular" aria-describedby="basic-addon1" style="max-width:100%" required>
                                </div>
                                <span style="color:red" ng-show="myForm.TelCel.$invalid" />
                                <span ng-show="myForm.TelCel.$error.required">*Teléfono Celular requerido</span>
                            </div>
                            <div class="col-md-6 py-2">
                                <div class="input-group mb-3">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text" id="basic-addon1">Teléfono Adicional</span>
                                    </div>
                                    <input ng-model="ctrl.TelAd" id="TelAd" name="TelAd" type="text" ng-change="ctrl.GlobalInfoPersonal()" class="form-control" placeholder="Teléfono Adicional" aria-label="TeléfonoAdicional" aria-describedby="basic-addon1" style="max-width:100%" required>
                                </div>
                                <span style="color:red" ng-show="myForm.TelAd.$invalid" />
                                <span ng-show="myForm.TelAd.$error.required">*Teléfono Adicional requerido</span>
                            </div>
                            <div class="col-md-12 py-2">
                                <div class="input-group mb-3">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text" id="basic-addon1">E-mail</span>
                                    </div>
                                    <input ng-model="ctrl.Email" id="Email" name="Email" type="text" ng-change="ctrl.GlobalInfoPersonal()" class="form-control" placeholder="E-mail" aria-label="Email" aria-describedby="basic-addon1" style="max-width:42.5%" required>
                                </div>
                                <span style="color:red" ng-show="myForm.Email.$invalid" />
                                <span ng-show="myForm.Email.$error.required">*E-mail requerido</span>
                            </div>
                            <div class="col-md-6">
                                <label>Genero</label>
                                <div class="form-check">
                                    <input class="form-check-input" ng-model="ctrl.SexoM" type="radio" name="exampleRadios" id="exampleRadios1" value="option1" checked>
                                    <label class="form-check-label" for="exampleRadios1">
                                        Masculino
                                    </label>
                                    <br />
                                    <input class="form-check-input" ng-model="ctrl.SexoF" type="radio" name="exampleRadios" id="exampleRadios1" value="option1" checked>
                                    <label class="form-check-label" for="exampleRadios1">
                                        Femenino
                                    </label>
                                </div>
                            </div>
                            <div class="col-md-12 py-2">
                                <div class="input-group mb-3">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text" id="basic-addon1">Área de Interes</span>
                                    </div>
                                    <input ng-model="ctrl.AreaInt" id="AreaInt" name="AreaInt" type="text" ng-change="ctrl.GlobalInfoPersonal()" class="form-control" placeholder="Área de Interes" aria-label="ÁreaInteres" aria-describedby="basic-addon1" style="max-width:100%" required>
                                </div>
                                <span style="color:red" ng-show="myForm.AreaInt.$invalid" />
                                <span ng-show="myForm.AreaInt.$error.required">*Área de Interes requerida</span>
                            </div>
                            <div class="col-md-12 py-2">
                                <div class="input-group mb-3">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text" id="basic-addon1">Objetivo Personal</span>
                                    </div>
                                    <textarea ng-model="ctrl.ObjPersonal" id="ObjPersonal" name="ObjPersonal" type="text" ng-change="ctrl.GlobalInfoPersonal()" class="form-control" aria-label="ObjetivoPersonal" aria-describedby="basic-addon1" style="max-width:100%; max-height:70%; min-height:70%" required></textarea>
                                </div>
                                <span style="color:red" ng-show="myForm.ObjPersonal.$invalid" />
                                <span ng-show="myForm.ObjPersonal.$error.required">*Objetivo Personal requerido</span>
                            </div>
                            <div class="col-md-1">
                                <input id="informacionEmpresa" type="button" class="btn btn-info" value="Continuar"
                                       ng-disabled="myForm.Matricula.$invalid || myForm.NombreCandidato.$invalid || myForm.ApePatCandidato.$invalid
                                       || myForm.ApeMatCandidato.$invalid || myForm.FecNac.$invalid || myForm.Estado.$invalid
                                       || myForm.Municipio.$invalid || myForm.Domicilio.$invalid || myForm.TelCel.$invalid|| myForm.TelAd.$invalid
                                       || myForm.Email.$invalid|| myForm.AreaInt.$invalid|| myForm.ObjPersonal.$invalid" 
                                       ng-click="ctrl.GuardarInformacionCandidato()"/>
                            </div>

                        </div>
                    </form>
                </div>

                <div class="tab-pane fade" id="estAcad" role="tabpanel" aria-labelledby="profile-tab">
                    <form name="myForm2">
                        <div class="row">
                            <div class="col-md-6 py-3">
                                <div class="input-group mb-3">
                                    <div class="input-group-prepend">
