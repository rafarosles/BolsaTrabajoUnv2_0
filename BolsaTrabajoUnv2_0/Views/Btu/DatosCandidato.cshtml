                                        <span class="input-group-text" id="basic-addon1">Software</span>
                                    </div>
                                    <input id="Software" name="Software" ng-model="ctrl.Software" type="text" ng-change="" class="form-control" placeholder="Software" aria-label="Software" aria-describedby="basic-addon1" style="max-width:100%" required>
                                </div>
